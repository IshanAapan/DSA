
//  Prefix sum 1D-Array :- 1 indexing/ 1 indexing

#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,q;
    cin>>n>>q;
    vector<int> v(n+1);
    vector<int> pre(n+1,0);
    
    for(int i=1;i<=n;i++){
        cin>>v[i];
        pre[i]=pre[i-1]+v[i];
    }
    
    for(int i=1;i<=q;i++){
        int l,r;
        cin>>l>>r;
        cout<<pre[r]-pre[l-1]<<endl;
    }
    
    return 0;
}
